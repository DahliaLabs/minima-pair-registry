{
  "name": "@dahlia-labs/minima-pair-registry",
  "version": "0.0.0",
  "description": "Registry of all pairs used in Minima router",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/DahliaLabs/minima-pair-registry"
  },
  "keywords": [
    "celo",
    "blockchain"
  ],
  "author": "Kyle Scott <kyle@dahlia.finance>",
  "license": "AGPL-3.0",
  "packageManager": "yarn@3.2.0",
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.3",
    "@saberhq/eslint-config": "^1.13.6",
    "@types/bn.js": "^5.1.0",
    "@types/eslint": "^8.4.2",
    "@types/lodash": "^4.14.182",
    "@types/node": "17.0.34",
    "@types/prettier": "^2.6.1",
    "eslint": "^8.15.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "ts-node": "10.7.0",
    "typescript": "4.6.4"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist/",
    "src/"
  ],
  "dependencies": {
    "@dahlia-labs/celo-contrib": "^0.4.2",
    "@dahlia-labs/celo-tokens": "^0.4.2",
    "@dahlia-labs/mobius-config-registry": "^0.0.0",
    "@dahlia-labs/token-utils": "^0.4.2",
    "@ethersproject/abi": "^5.6.2",
    "@ethersproject/address": "^5.6.0",
    "@ethersproject/bignumber": "^5.6.1",
    "@ethersproject/bytes": "^5.6.1",
    "@ethersproject/constants": "^5.6.0",
    "@ethersproject/contracts": "^5.6.1",
    "@ethersproject/providers": "^5.6.6",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/schema-ast": "^2.4.1",
    "@graphql-codegen/typescript": "^2.4.11",
    "@graphql-codegen/typescript-operations": "^2.4.0",
    "@saberhq/tsconfig": "^1.13.6",
    "@typechain/ethers-v5": "^10.0.0",
    "@yarnpkg/doctor": "^4.0.0-rc.6",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "axios": "^0.27.2",
    "bignumber.js": "^9.0.2",
    "bn.js": "^5.2.0",
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.1.5",
    "ethers": "^5.6.6",
    "graphql": "^16.5.0",
    "graphql-codegen": "^0.4.0",
    "graphql-tag": "^2.12.6",
    "jsbi": "^4.3.0",
    "lodash": "^4.17.21",
    "node-fetch": "^3.2.4",
    "tiny-invariant": "^1.2.0",
    "tslib": "^2.4.0",
    "typechain": "^8.0.0",
    "typedoc": "^0.22.15"
  },
  "scripts": {
    "codegen:local": "graphql-codegen",
    "pull-schema": "graphql-codegen -c codegen-schema.yml",
    "registry:sync": "cross-env TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' ts-node src/scripts/fetch.ts",
    "registry:test": "cross-env TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' ts-node src/index.ts",
    "typechain": "typechain --target=ethers-v5 'src/abis/*.json' --out-dir src/generated/",
    "build": "tsc && tsc --project tsconfig.cjs.json",
    "clean": "rm -fr dist/",
    "typecheck": "tsc --project tsconfig.cjs.json",
    "lint": "eslint . --cache",
    "lint:ci": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "docs:generate": "typedoc --excludePrivate --includeVersion --out site/ts/ src/index.ts",
    "prepare": "husky install",
    "doctor:packages": "yarn doctor packages/"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{md,json,js,yml,yaml}": "prettier --write"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/DahliaLabs/minima-pair-registry#readme"
}
